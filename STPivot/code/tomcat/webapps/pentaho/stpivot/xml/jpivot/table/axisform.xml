<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE xform [
<!ENTITY nbsp "&#160;">
<!ENTITY euro "&#8364;">
]>

<xform style="manual">
	
	<table border="0" cellspacing="0">
		<tr>
			<th>&nbsp;</th>
			<th>Row Axis</th>
			<th>Column Axis</th>
		</tr>
		<tr>
			<td align="right">Show Parents</td>
			<td align="center"><checkBox modelReference="rowAxisBuilder.axisConfig.showParentMembers"/></td>
			<td align="center"><checkBox modelReference="columnAxisBuilder.axisConfig.showParentMembers"/></td>
		</tr>
		<tr>
			<td align="right">Indent Members:</td>
			<td align="center"><checkBox modelReference="rowAxisBuilder.axisConfig.memberIndent"/></td>
			<td align="center"><checkBox modelReference="columnAxisBuilder.axisConfig.memberIndent"/></td>
		</tr>
		<tr>
			<td align="right">Hierarchy Header:</td>
			<td align="center">
				<listBox1 type="int" modelReference="rowAxisBuilder.axisConfig.hierarchyHeader">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies"/>
					<listItem value="2" label="levels"/>
				</listBox1>
			</td>
			<td align="center">
				<listBox1 type="int" modelReference="columnAxisBuilder.axisConfig.hierarchyHeader">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies"/>
					<listItem value="2" label="levels"/>
				</listBox1>
			</td>
		</tr>
		<tr>
			<td align="right">Row Header:</td>
			<td align="center">
				<listBox1 type="int" modelReference="rowAxisBuilder.axisConfig.positionHeader">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies"/>
					<listItem value="2" label="levels"/>
				</listBox1>
			</td>
			<td align="center">&nbsp;</td>
		</tr>
		<tr>
			<td align="right">Member Span:</td>
			<td align="center">
				<listBox1 type="int" modelReference="rowAxisBuilder.axisConfig.memberSpan">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies only"/>
					<listItem value="2" label="positions only"/>
					<listItem value="3" label="hierarchies then position"/>
					<listItem value="4" label="positions then hierarchies"/>
				</listBox1>
			</td>
			<td align="center">
				<listBox1 type="int" modelReference="columnAxisBuilder.axisConfig.memberSpan">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies only"/>
					<listItem value="2" label="positions only"/>
					<listItem value="3" label="hierarchies then position"/>
					<listItem value="4" label="positions then hierarchies"/>
				</listBox1>
			</td>
		</tr>
		<tr>
			<td align="right">Header Span:</td>
			<td align="center">
				<listBox1 type="int" modelReference="rowAxisBuilder.axisConfig.headerSpan">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies only"/>
					<listItem value="2" label="positions only"/>
					<listItem value="3" label="hierarchies then position"/>
					<listItem value="4" label="positions then hierarchies"/>
				</listBox1>
			</td>
			<td align="center">
				<listBox1 type="int" modelReference="columnAxisBuilder.axisConfig.headerSpan">
					<listItem value="0" label="nothing"/>
					<listItem value="1" label="hierarchies only"/>
					<listItem value="2" label="positions only"/>
					<listItem value="3" label="hierarchies then position"/>
					<listItem value="4" label="positions then hierarchies"/>
				</listBox1>
			</td>
		</tr>
		<tr>
			<td align="right" colspan="3">
				<imgButton src="/stpivot/style/wcf/form/ok.png" action="validate" label="OK" handler="com.tonbeller.wcf.form.ButtonHandler" hide="false"/>
				<imgButton src="/stpivot/style/wcf/form/cancel.png" action="revert" label="Cancel" handler="com.tonbeller.wcf.form.ButtonHandler" hide="false"/>
			</td>
		</tr>
	</table>

</xform>
